{
    "name": "Barcode",
    "version": "1.0.0",
    "description": "Express+React",
    "main": "src/server/index.js",
    "scripts": {
        "dec": "openssl enc -aes-256-cbc -d -in .env.encrypted > .env",
        "enc": "openssl enc -aes-256-cbc -in .env > .env.encrypted",
        "start": "node -r esm src/server/index.js",
        "build": "webpack --env.production",
        "client": "webpack-dev-server --hot --color --progress --env.development -d",
        "server": "nodemon -r esm --watch src/server --inspect src/server/index.js",
        "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
    },
    "author": "Pavel Ravits",
    "license": "ISC",
